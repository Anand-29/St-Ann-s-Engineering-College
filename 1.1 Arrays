1.Maximum and minimum element

l=list(map(int,input().split()))
min,max=l[0],l[0]
for i in range(1,len(l)):
    if(l[i]>max):
        max=l[i]
    if(l[i]<min):
        min=l[i]
print(min,max)

2.Same or not

def solution(l1,l2):
    if(len(l1)!=len(l2)):
        return False
    for i in range(len(l1)):
        if(l1[i]!=l2[i]):
            return False
    return True
l1=list(map(int,input().split()))
l2=list(map(int,input().split()))
print(solution(l1,l2))

3.Sum of odd and even numbers

l=list(map(int,input().split()))
oddsum,evensum=0,0
for i in range(len(l)):
    if(l[i]%2==0):
        evensum=evensum+l[i]
    else:
        oddsum+=l[i]
print(evensum,oddsum)


4.Add two arrays

def solution(l1,l2):
    if(len(l1)!=len(l2)):
        return False
    for i in range(len(l1)):
        l1[i]=l1[i]+l2[i]
    return l1;
l1=list(map(int,input().split()))
l2=list(map(int,input().split()))
print(solution(l1,l2))

5.Search in an array

l=list(map(int,input("Enter data:").split()))
searchdata=int(input("Enter search data: "))
f=0
for i in range(len(l)):
    if(l[i]==searchdata):
        f=1
        break
if(f==0):
    print(searchdata,"is not present")
else:
    print(searchdata,"is present")

6.Print sorted array

l=list(map(int,input().split()))
for i in range(len(l)):
    for j in range(len(l)):
        if(l[i]<l[j]):
            t=l[i]
            l[i]=l[j]
            l[j]=t      #l[i],l[j]=l[j],l[i]
print(*l)

