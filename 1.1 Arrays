1.Maximum and minimum element

l=list(map(int,input().split()))
min,max=l[0],l[0]
for i in range(1,len(l)):
    if(l[i]>max):
        max=l[i]
    if(l[i]<min):
        min=l[i]
print(min,max)

2.Same or not

def solution(l1,l2):
    if(len(l1)!=len(l2)):
        return False
    for i in range(len(l1)):
        if(l1[i]!=l2[i]):
            return False
    return True
l1=list(map(int,input().split()))
l2=list(map(int,input().split()))
print(solution(l1,l2))

